#!/bin/env fish

echo nightjob

# set display
set -x DISPLAY :0

# axel
mkdir -p ~/Downloads/nightjob
cd ~/Downloads/nightjob
mkdir -p log
cp list log/

set -l downloads (cat list)
for i in (seq (count $downloads))
    echo $downloads[$i]
    axel $downloads[$i] -v > log/$i ^&1
end

# transmission
transmission-gtk
